{
  "name": "is-social",
  "version": "1.0.4",
  "description": "Validators for social media profiles.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && node node_modules/mocha/bin/mocha test --recursive",
    "lint": "node node_modules/eslint/bin/eslint.js lib",
    "doc": "jsdoc lib -r -p -P package.json -d doc",
    "cover": "node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- test --recursive",
    "travis": "node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha --report lcovonly -- test --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WatchBeam/is-social.git"
  },
  "keywords": [
    "social",
    "profile",
    "validator"
  ],
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WatchBeam/is-social/issues"
  },
  "homepage": "https://github.com/WatchBeam/is-social",
  "devDependencies": {
    "chai": "^2.3.0",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1"
  }
}
